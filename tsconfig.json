{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "esModuleInterop": true,
    "baseUrl": "./",
    "outDir": "./dist",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["cypress"]
  },
  "include": [
    "**/*.ts",
  ],
  "exclude": [
    "node_modules",
    "cypress",
    "**/.*/",
    "cypress.config.ts"
  ]
}